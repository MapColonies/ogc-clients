{
  "name": "@map-colonies/wfs-client",
  "version": "1.0.0",
  "description": "This is template for map colonies typescript packages",
  "main": "./dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:unit:debug": "jest --config=./tests/configurations/unit/jest.config.js --debug",
    "test": "tsc --noEmit -p tsconfig.json && jest --config=./tests/configurations/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean",
    "build_old": "tsc --project tsconfig.build.json",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/ts-npm-package-boilerplate.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/ts-npm-package-boilerplate/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/MapColonies/ts-npm-package-boilerplate#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@commitlint/cli": "^19.6.1",
    "@map-colonies/eslint-config": "^4.0.0",
    "@map-colonies/prettier-config": "^0.0.1",
    "@map-colonies/tsconfig": "^1.0.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^26.0.24",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^14.14.12",
    "babel-jest": "^29.7.0",
    "commitlint": "^19.6.1",
    "eslint": "^8.36.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "rollup": "^2.6.1",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-license": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@map-colonies/types": "^1.6.0",
    "@types/geojson": "^7946.0.16",
    "jsdom": "22.0.0",
    "jsonix": "^3.0.0",
    "lodash-es": "^4.17.21",
    "ogc-schemas": "^2.6.1",
    "ol": "^10.4.0",
    "patch-package": "^6.2.2",
    "ts-jest": "^29.3.2"
  }
}
